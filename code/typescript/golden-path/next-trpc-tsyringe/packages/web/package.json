{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build && mkdir -p .turbo-docker && echo $TURBO_HASH > .turbo-docker/hash",
    "containerize": "turbo-scripts $DOCKER_IMAGE_REPOSITORY",
    "check": "biome check .",
    "check:unsafe": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbo",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "run-swc-esm": "node --require dotenv/config --import @swc-node/register/esm-register",
    "cli": "yarn run-swc-esm ./src/cli.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.879.0",
    "@ewnd9/turbo-scripts": "^0.2.3",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.11.0",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.41.0",
    "next": "^15.2.3",
    "node-fetch": "^3.3.2",
    "pg-boss": "^10.3.2",
    "postgres": "^3.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "socks-proxy-agent": "^8.0.5",
    "superjson": "^2.2.1",
    "tsyringe": "^4.10.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@playwright/test": "1.55.0",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.13.5",
    "@tailwindcss/postcss": "^4.0.15",
    "@testcontainers/postgresql": "^11.5.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "dotenv": "^17.2.2",
    "nanoid": "^5.1.5",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.15",
    "testcontainers": "^11.5.1",
    "tsx": "^4.19.0",
    "typescript": "^5.8.2",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  }
}
